<%= page_title("Your Properties") %>


<%= page_header "Your Properties" do %>
  <%= link_to 'Create Property', new_member_property_path, class: 'btn btn-info' %>
<% end %>


<%= paginate @properties %>

<ul class='no-space'>
  <% @properties.each do |property| %>
    <%= render 'shared/properties/property', property: property %>
  <% end %>
</ul>

<%= paginate @properties %>